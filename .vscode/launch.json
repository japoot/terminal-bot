{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Japoot: Main",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "run start:main --inspect=5858 --remote-debugging-port=9223"
      ],
      "preLaunchTask": "Start Webpack Dev"
    },
    {
      "name": "Japoot: Renderer",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}",
      "timeout": 15000
    }
  ],
  "compounds": [
    {
      "name": "Japoot: All",
      "configurations": ["Japoot: Main", "Japoot: Renderer"]
    }
  ]
}
